{
  "name": "phoenix-burstcoin-web-wallet",
  "author": {
    "name": "Burst Apps Team (BAT)",
    "email": "contact@burst-coin.org"
  },
  "productName": "Phoenix Burstcoin Wallet",
  "description": "The Burstcoin web wallet",
