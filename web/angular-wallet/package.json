  "scripts": {
    "bump-version": "node ../../scripts/version.js --target ng",
    "ng": "ng",
    "start": "ng serve --open --proxy-config proxy.config.json",
    "start-hmr": "ng serve --configuration hmr --source-map=false --hmr-warning=false",
    "start-hmr-sourcemaps": "ng serve --configuration hmr --source-map=true --hmr-warning=false",
    "build": "cross-env NODE_OPTIONS=--max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng build",
    "build:ci": "npm run build -- --aot --source-map=false --stats-json",
    "build:stats": "npm run build -- --dev --stats-json",
    "build:prod": "npm run build -- --prod",
    "build:web": "npm run build -- --configuration=web",
    "build:prod-stats": "npm run build -- --prod --stats-json",
    "test": "jest",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "bundle-report": "webpack-bundle-analyzer dist/stats.json",
    "test:watch": "jest --watch"
  },
  "private": true,
